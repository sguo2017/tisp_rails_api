<%- model_class = SysMsgsTimeline -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:sys_msg_id) %>:</strong></dt>
  <dd><%= @sys_msgs_timeline.sys_msg_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @sys_msgs_timeline.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= @sys_msgs_timeline.status %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              sys_msgs_timelines_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_sys_msgs_timeline_path(@sys_msgs_timeline), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              sys_msgs_timeline_path(@sys_msgs_timeline),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
