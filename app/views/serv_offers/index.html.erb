<p id="notice"><%= notice %></p>

<h1>Serv Offers</h1>

<table>
  <thead>
    <tr>
      <th>Serv title</th>
      <th>Serv detail</th>
      <th>Serv imges</th>
      <th>Serv catagory</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @serv_offers.each do |serv_offer| %>
      <tr>
        <td><%= serv_offer.serv_title %></td>
        <td><%= serv_offer.serv_detail %></td>
        <td><%= serv_offer.serv_imges %></td>
        <td><%= serv_offer.serv_catagory %></td>
        <td><%= link_to 'Show', serv_offer %></td>
        <td><%= link_to 'Edit', edit_serv_offer_path(serv_offer) %></td>
        <td><%= link_to 'Destroy', serv_offer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Serv Offer', new_serv_offer_path %>
