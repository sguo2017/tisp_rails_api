<%- model_class = ServOffer -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:serv_title) %>:</strong></dt>
  <dd><%= @serv_offer.serv_title %></dd>
  <dt><strong><%= model_class.human_attribute_name(:serv_detail) %>:</strong></dt>
  <dd><%= @serv_offer.serv_detail %></dd>
  <dt><strong><%= model_class.human_attribute_name(:serv_imges) %>:</strong></dt>
  <dd><%= @serv_offer.serv_imges %></dd>
  <dt><strong><%= model_class.human_attribute_name(:serv_catagory) %>:</strong></dt>
  <dd><%= @serv_offer.serv_catagory %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @serv_offer.user_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              serv_offers_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_serv_offer_path(@serv_offer), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              serv_offer_path(@serv_offer),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
